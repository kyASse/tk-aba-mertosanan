{
  "$schema": "https://unpkg.com/knip@latest/schema.json",
  "entry": [
    "app/**/page.@(ts|tsx)",
    "app/**/layout.@(ts|tsx)",
    "app/**/route.@(ts|tsx)",
    
    "next.config.*"
  ],
  "project": [
    "app/**/*.@(ts|tsx)",
    "components/**/*.@(ts|tsx)",
    "lib/**/*.@(ts|tsx)",
  "hooks/**/*.@(ts|tsx)"
    
  ],
  "ignore": [
    "**/*.d.ts",
    "**/*.stories.@(ts|tsx)",
    "**/__tests__/**",
    "supabase/migrations/**",
    "public/**"
  ],
  "ignoreDependencies": [
    "eslint",
    "eslint-config-next"
  ]
}
